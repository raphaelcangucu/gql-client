{
  "name": "raphaelcangucu/gql-client",
  "version": "0.2.0",
  "description": "Graphql Client for Laravel",
  "main": "index.js",
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bendeckdavid/graphql-laravel-client.git"
  },
  "author": {
    "email": "david@bendeck.dev",
    "name": "David Gutierrez",
    "github": "BendeckDavid"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bendeckdavid/graphql-laravel-client/issues"
  },
  "homepage": "https://github.com/bendeckdavid/graphql-laravel-client#readme",
  "keywords": [
    "Laravel",
    "Graphql",
    "Graphql Client",
    "BendeckDavid"
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/config": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0|^11.0",
    "mockery/mockery": "^1.4",
    "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "RaphaelCangucu\\GqlClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RaphaelCangucu\\GqlClient\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel":{
      "providers": [
        "RaphaelCangucu\\GqlClient\\GraphqlClientServiceProvider"
      ]
    }
  }
}
